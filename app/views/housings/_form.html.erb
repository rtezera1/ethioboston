<%= form_for(@housing) do |f| %>

  <% if @housing.errors.full_messages.count > 0 %>
    <ul>
      <% @housing.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  <% end %>

  <div>
    <%= f.label :type_of_housing %>
    <%= f.text_field :type_of_housing %>
  </div>
  <div>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>
  <div>
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>
  <div>
    <%= f.label :price %>
    <%= f.text_field :price %>
  </div>
  <div>
    <%= f.label :availability_date %>
    <%= f.text_field :availability_date %>
  </div>
  <div>
    <%= f.label :contact_number %>
    <%= f.text_field :contact_number %>
  </div>
  <div>
    <%= f.label :contact_email %>
    <%= f.text_field :contact_email %>
  </div>
  <div>
    <%= f.file_field :avatar %>
  <div>
     <%= recaptcha_tags %>
  </div>
  </div>
  <div class='button expand'>
    <%= f.submit %>
  </div>
<% end %>
